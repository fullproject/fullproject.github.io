RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^process?$ process.php [NC,L]
RewriteRule ^error?$ error.php [NC,L]

